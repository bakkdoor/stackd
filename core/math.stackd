in: core.math ;

: double ! ( x -- 2x )
    2 * ;

: sq ! ( x -- x*x )
    dup * ;

: even? ! ( x -- ? )
    2 mod 0 = ;

: odd? ! ( x -- ? )
    even? not ;

: sum ! ( seq -- x )
    0 [ + ] reduce ;

: product ! ( seq -- x )
    1 [ * ] reduce ;

: 1- ! ( x -- y)
    1 - ;

: 1+ ! ( x -- y )
    1 + ;

: neg ! ( x - -x )
    0 swap - ;